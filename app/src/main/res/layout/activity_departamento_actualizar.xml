<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".departamento.DepartamentoActualizarActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="296dp"
        android:layout_height="52dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="51dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="276dp"
        android:text="Actualizar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editNombreDepartamento"
        android:onClick="actualizarDepartamento"/>

    <EditText
        android:id="@+id/editIdDepartamento"
        android:layout_width="263dp"
        android:layout_height="49dp"
        android:layout_marginStart="74dp"
        android:layout_marginTop="234dp"
        android:layout_marginEnd="74dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editNombreDepartamento"
        android:layout_width="259dp"
        android:layout_height="49dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="76dp"
        android:layout_marginBottom="51dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editIdDepartamento" />

</androidx.constraintlayout.widget.ConstraintLayout>